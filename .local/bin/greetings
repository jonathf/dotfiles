#!/bin/bash

if [[ -f /etc/os-release ]]; then
    name=$(cat /etc/os-release | grep "PRETTY_NAME" | sed -e 's/PRETTY_NAME="//' | sed -e 's/"//')
else
    name="Android Linux"
fi
echo -en "\e[1;30;41m1\e[1;30;101m9\e[0m "
echo -en "\e[1;30;42m2\e[1;30;102mA\e[0m "
echo -en "\e[1;30;43m3\e[1;30;103mB\e[0m "
echo -en "\e[1;30;44m4\e[1;30;104mC\e[0m "
echo -en "\e[1;30;45m5\e[1;30;105mD\e[0m "
echo -en "\e[1;30;46m6\e[1;30;106mE\e[0m "
echo -en "\e[1;30;47m7\e[1;30;107mF\e[0m "
echo -en "$name"
echo -e "\e[0m"
